{
    "name": "entrolytics-ng/php",
    "version": "2.0.0",
    "description": "PHP SDK for Entrolytics - First-party growth analytics",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "entrolytics",
        "entrolytics-ng",
        "analytics",
        "tracking",
        "web-analytics",
        "laravel"
    ],
    "homepage": "https://ng.entrolytics.click",
    "support": {
        "issues": "https://github.com/entrolytics/entrolytics-ng/issues",
        "source": "https://github.com/entrolytics/entrolytics-ng/tree/main/packages/php"
    },
    "authors": [
        {
            "name": "Entrolytics",
            "email": "hey@ng.entrolytics.click"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "EntrolyticsNG\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EntrolyticsNG\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EntrolyticsNG\\Laravel\\EntrolyticsServiceProvider"
            ],
            "aliases": {
                "EntrolyticsNG": "EntrolyticsNG\\Laravel\\EntrolyticsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
